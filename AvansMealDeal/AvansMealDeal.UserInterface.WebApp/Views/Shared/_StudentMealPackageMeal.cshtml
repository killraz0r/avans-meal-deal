@using AvansMealDeal.Domain.Models
@using AvansMealDeal.UserInterface.WebApp.Helpers
@model Meal

<tr>
    <td style="height: 52px">
        @if (@Model.Photo != null)
        {
            <img src="data:image/png;base64,@Convert.ToBase64String(Model.Photo)" style="width:50px; height:50px;" />
        }
    </td>
    <td>@Model.Name</td>
    <td>@HtmlHelpers.DisplayDutchBoolean(Model.ContainsAlcohol)</td>
</tr>